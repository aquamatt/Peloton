[profile]
  cpus=1
  ram=1024
  # Accept RPC via Twisted or message bus
  rpc=pb,bus
  
[plugins]
 [[amqp-qpid]]
   enabled=True

 [[shell]]
   enabled=True

 [[cockpit]]
   enabled=False
   
 [[backpack]]
   enabled=True

 [[bambi]]
   comment=Bambi for testing ONLY!!
   class=peloton.plugins.bambi.BambiPlugin
   enabled=False
   
 [[pseudomq]]
   name=eventbus
   comment=PseudoMQ test harness for testing ONLY
   class=peloton.plugins.pseudomq.PseudoMQ
   host=127.0.0.1:9111
   enabled=False